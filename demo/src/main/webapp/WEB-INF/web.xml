<web-app>
  <display-name>myyi-mvc</display-name>
  <servlet>
    <servlet-name>DispatcherServlet</servlet-name>
  	<servlet-class>com.enjoy.mayi.servlet.DispatcherServlet</servlet-class>
  	<load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
